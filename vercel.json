{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "rewrites": [
        {
            "source": "/",
            "destination": "/api/index.php"
        },
        {
            "source": "/style.css",
            "destination": "/style.css"
        },
        {
            "source": "/init/api/cloud",
            "destination": "/api/init_cloud.php"
        },
        {
            "source": "/(init_cloud.php)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/api/$1"
        }
    ]
}